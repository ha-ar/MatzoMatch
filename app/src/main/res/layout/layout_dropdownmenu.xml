<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        >
        <com.mikhaellopez.circularimageview.CircularImageView
            android:id="@+id/user_image"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:src="@drawable/profile_sky"
            app:border_color="#EEEEEE"
            android:layout_gravity="center_horizontal"
            app:border_width="5dp"
            app:shadow="true"
            android:visibility="visible"/>

    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:background="#88676767"
        android:layout_height="1dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:id="@+id/layout_list"
        android:layout_height="50dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/search_nav"
            android:id="@+id/home_image" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Discovery Preferences"
            android:textColor="#00a3ca"
            android:id="@+id/List"
            android:paddingLeft="20dp"
            android:layout_gravity="center_vertical"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:background="#88676767"
        android:layout_height="1dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:id="@+id/layout_internal"
        android:layout_height="50dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/heart"
            android:id="@+id/internal_image" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="See Who Liked You"
            android:textColor="#00a3ca"
            android:id="@+id/internal"
            android:paddingLeft="20dp"
            android:layout_gravity="center_vertical"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:background="#88676767"
        android:layout_height="1dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:id="@+id/layout_external"
        android:layout_height="50dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/settings"
            android:id="@+id/external_image" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Application Settings"
            android:textColor="#00a3ca"
            android:id="@+id/external"
            android:paddingLeft="20dp"
            android:layout_gravity="center_vertical"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:background="#88676767"
        android:layout_height="1dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:id="@+id/layout_profile"
        android:layout_height="50dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/contact"
            android:id="@+id/category_image" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Conatct Matzomatch"
            android:textColor="#00a3ca"
            android:id="@+id/category"
            android:paddingLeft="20dp"
            android:layout_gravity="center_vertical"/>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:background="#88676767"
        android:layout_height="1dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:id="@+id/layout_explorar"
        android:layout_height="50dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:layout_gravity="center_vertical"
            android:background="@drawable/share"
            android:id="@+id/favorite_image" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Share Matzomatch"
            android:textColor="#00a3ca"
            android:id="@+id/Share"
            android:paddingLeft="20dp"
            android:layout_gravity="center_vertical"/>
    </LinearLayout>



</LinearLayout>